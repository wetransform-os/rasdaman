{
  "config": {
    "service_url": "PETASCOPE_URL",
    "tmp_directory": "/tmp/",
    "mock": false,
    "automated": true,
    "track_files": false
  },
  "input": {
    "coverage_id": "test_alaska_epsg_3338",
    "paths": [
      "*.tif"
    ]
  },
  "recipe": {
    "name": "general_coverage",
    "options": {
      "wms_import": true,
      "coverage": {
        "crs": "EPSG/0/3338",
        "metadata": {
          "type": "xml",
          "global": {
            "Title": ""
          }
        },
        "slicer": {
          "type": "gdal",
          "axes": {
            "X": {
              "min": "${gdal:minX}",
              "max": "${gdal:maxX}",
              "resolution": "${gdal:resolutionX}"
            },
            "Y": {
              "min": "${gdal:minY}",
              "max": "${gdal:maxY}",
              "resolution": "${gdal:resolutionY}"
            }
          }
        }
      }
    }
  }
}

