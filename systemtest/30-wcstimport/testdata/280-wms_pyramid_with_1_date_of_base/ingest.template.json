{
  "config": {
    "service_url": "PETASCOPE_URL",
    "tmp_directory": "/tmp/",
    "mock": false,
    "track_files": false,
    "automated": true
  },
  "input": {
    "coverage_id": "test_wms_pyramid_with_1_date_of_base",
    "paths": [
      "*.tif"
    ]
  },
  "recipe": {
    "name": "time_series_irregular",
    "options": {
      "pyramid_bases": ["test_wms_base_with_2_dates"],
      "wms_import": true,
      "time_parameter": {
        "filename": {
          "regex": "(.*)_(.*)\\.(.*)",
          "group": "1"
        },
        "datetime_format": "YYYYMMDD"
      },
      "time_crs": "SECORE_URL/crs/OGC/0/AnsiDate",
      "tiling": "ALIGNED [0:0, 0:999, 0:999]"
    }
  }
}
