{
  "config": {
    "service_url": "PETASCOPE_URL",
    "tmp_directory": "/tmp/",
    "mock": false,
    "track_files": false,
    "automated": true
  },
  "input": {
    "coverage_id": "test_wcs_3D_Timeseries_Irregular_CRS_shorthand",
    "paths": [
      "SCALE*.TIFF"
    ]
  },
  "recipe": {
    "name": "time_series_irregular",
    "options": {
      "import_order": "descending",
      "time_parameter": {
        "filename": {
          "regex": "(.*)_(.*)\\.(.*)",
          "group": "2"
        },
        "datetime_format": "YYYY-MM"
      },
      "time_crs": "OGC:AnsiDate?axis-label=\"unix\"",
      "tiling": "ALIGNED [0:0, 0:999, 0:999] TILE SIZE 3000000"
    }
  }
}
