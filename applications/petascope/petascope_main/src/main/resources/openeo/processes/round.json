        },
        {
            "rel": "about",
            "href": "https://ieeexplore.ieee.org/document/8766229",
            "title": "IEEE Standard 754-2019 for Floating-Point Arithmetic"
        }
    ],
    "wcps": "round(<x>)",
    "categories": [
        "Arithmetic"
    ]
}